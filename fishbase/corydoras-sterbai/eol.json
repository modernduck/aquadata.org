aURL":"http://upload.wikimedia.org/wikipedia/commons/d/d2/Corydoras_sterbai_02.jpg","eolMediaURL":"http://media.eol.org/content/2012/06/14/22/73267_orig.jpg","eolThumbnailURL":"http://media.eol.org/content/2012/06/14/22/73267_98_68.jpg","agents":[{"full_name":"Tino Strauss","homepage":"http://commons.wikimedia.org/wiki/User:TinoStrauss","role":"photographer"},{"full_name":"Wikimedia Commons","homepage":"http://commons.wikimedia.org/","role":"provider"}],"references":[]},{"identifier":"6ad4c9a2d62855ca2fee479302959f65","dataObjectVersionID":26845365,"dataType":"http://purl.org/dc/dcmitype/StillImage","dataSubtype":"","vettedStatus":"Unreviewed","dataRating":2.5,"mimeType":"image/jpeg","title":"File:55067369.Corydorassterbai.jpeg","language":"en","license":"http://creativecommons.org/publicdomain/mark/1.0/","source":"http://commons.wikimedia.org/wiki/File:55067369.Corydorassterbai.jpeg","description":"<b>English:</b> Corydoras sterbai, a highly sought after species of freshwater catfish for the home aquarium.&nbsp;&nbsp;<a href='http://commons.wikimedia.org/wiki/:en:Category:Siluriformes images'>Category:Siluriformes images</a>  <b>Norwegian:</b> <a href='http://commons.wikimedia.org/wiki/:no:Sterbas pansermalle'>Sterbas pansermalle</a>","mediaURL":"http://upload.wikimedia.org/wikipedia/commons/4/4f/55067369.Corydorassterbai.jpeg","eolMediaURL":"http://media.eol.org/content/2013/11/15/08/99882_orig.jpg","eolThumbnailURL":"http://media.eol.org/content/2013/11/15/08/99882_98_68.jpg","agents":[{"full_name":"Original uploader was Tsunamicarlos at en.wikipedia","homepage":"http://commons.wikimedia.org/wiki/:en:User:Tsunamicarlos","role":"photographer"},{"full_name":"Wikimedia Commons","homepage":"http://commons.wikimedia.org/","role":"provider"}],"references":[]},{"identifier":"31033dfabf8b558ce7388805797bd9f4","dataObjectVersionID":27074897,"dataType":"http://purl.org/dc/dcmitype/StillImage","dataSubtype":"","vettedStatus":"Unreviewed","dataRating":2.5,"mimeType":"image/j